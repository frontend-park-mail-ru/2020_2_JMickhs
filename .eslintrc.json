{
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "indent": [
            "error",
            4
        ]
    }
}