{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "target": "es5",
      "allowJs": true,
      "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@navbar/*": ["src/components/navbar/*"],
            "@eventBus/*": ["src/helpers/eventbus/*"],
            "@network/*": ["src/helpers/network/*"],
            "@user/*": ["src/helpers/user/*"],
            "@interfaces/*": ["src/helpers/interfaces/*"],
            "@hostel/*": ["src/components/hostel/*"],
            "@pageError/*": ["src/components/pageError/*"],
            "@router/*": ["src/helpers/router/*"],
            "@home/*": ["src/components/home/*"],
            "@list/*": ["src/components/list/*"],
            "@signin/*": ["src/components/signin/*"],
            "@signup/*": ["src/components/signup/*"],
            "@profile/*": ["src/components/profile/*"]
        },
    },
}